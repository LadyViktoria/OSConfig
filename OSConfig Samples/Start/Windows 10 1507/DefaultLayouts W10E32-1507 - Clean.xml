<?xml version="1.0" encoding="utf-8"?>
<FullDefaultLayoutTemplate 
    xmlns="http://schemas.microsoft.com/Start/2014/FullDefaultLayout"
    xmlns:start="http://schemas.microsoft.com/Start/2014/StartLayout"
    Version="1">
    <StartLayoutCollection>
		<!-- US and CEFIGS specific 6 cell wide Desktop layout with Preinstalled apps -->
		<!-- US and CEFIGS specific N-SKU 6 cell wide Desktop layout with Preinstalled apps -->
		<!-- US and CEFIGS specific 6 cell wide Desktop layout with No Preinstalled apps -->
        <!-- 6 cell wide Desktop layout with Preinstalled apps and no Cortana -->
		<!-- 6 cell wide Desktop layout N-SKU with Preinstalled apps and no Cortana -->
		<!-- 6 cell wide Desktop layout with no Preinstalled apps and no Cortana-->
        <StartLayout
            GroupCellWidth="6"
            PreInstalledAppsEnabled="false">
            <start:Group
                LocalizedNameResourceTag="TileGrid_DefaultGroup1">
                <start:Tile
                    AppUserModelID="microsoft.windowscommunicationsapps_8wekyb3d8bbwe!Microsoft.WindowsLive.Calendar"
                    Size="2x2"
                    Row="0"
                    Column="0"/>
                <start:Tile
                    AppUserModelID="microsoft.windowscommunicationsapps_8wekyb3d8bbwe!Microsoft.WindowsLive.Mail"
                    Size="4x2"
                    Row="0"
                    Column="2"/>
                <start:Tile
                    AppUserModelID="Microsoft.MicrosoftEdge_8wekyb3d8bbwe!MicrosoftEdge"
                    Size="4x2"
                    Row="2"
                    Column="0"/>
                <start:Tile
                    AppUserModelID="Microsoft.Windows.Photos_8wekyb3d8bbwe!App"
                    Size="2x2"
                    Row="2"
                    Column="4"/>
                <start:Tile
                    AppUserModelID="Microsoft.BingWeather_8wekyb3d8bbwe!App"
                    Size="2x2"
                    Row="4"
                    Column="0"/>
                <start:Tile
                    AppUserModelID="Microsoft.WindowsPhone_8wekyb3d8bbwe!CompanionApp.App"
                    Size="2x2"
                    Row="4"
                    Column="2"/>
                <start:Tile
                    AppUserModelID="Microsoft.Office.OneNote_8wekyb3d8bbwe!microsoft.onenoteim"
                    Size="2x2"
                    Row="4"
                    Column="4"/>
            </start:Group>
            <start:Group
                LocalizedNameResourceTag="TileGrid_DefaultGroup2">
                <start:Tile
                    AppUserModelID="Microsoft.XboxApp_8wekyb3d8bbwe!Microsoft.XboxApp"
                    Size="2x2"
                    Row="0"
                    Column="0"/>
                <start:Tile
                    AppUserModelID="Microsoft.ZuneMusic_8wekyb3d8bbwe!Microsoft.ZuneMusic"
                    Size="2x2"
                    Row="0"
                    Column="2"/>
                <start:Tile
                    AppUserModelID="Microsoft.ZuneVideo_8wekyb3d8bbwe!Microsoft.ZuneVideo"
                    Size="2x2"
                    Row="0"
                    Column="4"/>
                <start:Tile
                    AppUserModelID="Microsoft.BingFinance_8wekyb3d8bbwe!AppexFinance"
                    Size="2x2"
                    Row="2"
                    Column="0"/>
                <start:Tile
                    AppUserModelID="Microsoft.BingNews_8wekyb3d8bbwe!AppexNews"
                    Size="4x2"
                    Row="2"
                    Column="2"/>
                <start:Tile
                    AppUserModelID="Microsoft.WindowsStore_8wekyb3d8bbwe!App"
                    Size="4x2"
                    Row="4"
                    Column="0"/>
                <start:Tile
                    AppUserModelID="Microsoft.MicrosoftSolitaireCollection_8wekyb3d8bbwe!App"
                    Size="2x2"
                    Row="4"
                    Column="2"/>
                <start:Tile
                    AppUserModelID="Microsoft.MicrosoftOfficeHub_8wekyb3d8bbwe!Microsoft.MicrosoftOfficeHub"
                    Size="2x2"
                    Row="4"
                    Column="4"/>
            </start:Group>
        </StartLayout>
		<!-- Long Term Servicing Branch with Cortana -->
		<!-- Long Term Servicing Branch without Cortana -->
		<StartLayout
          GroupCellWidth="6"
          SKU="LongTermServicingBranch">
        <start:Group>
          <start:Tile
              AppUserModelID="Microsoft.MicrosoftEdge_8wekyb3d8bbwe!MicrosoftEdge"
              Size="2x2"
              Row="0"
              Column="0"/>
          <start:Tile
              AppUserModelID="Windows.ContactSupport_cw5n1h2txyewy!App"
              Size="2x2"
              Row="0"
              Column="2"/>
          <start:Tile
              AppUserModelID="Microsoft.WindowsFeedback_cw5n1h2txyewy!App"
              Size="2x2"
              Row="0"
              Column="4"/>
        </start:Group>
      </StartLayout>
    </StartLayoutCollection>
</FullDefaultLayoutTemplate>
